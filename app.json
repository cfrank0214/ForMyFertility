{
  "expo": {
    "name": "Fertility",
    "description": "This project is to support couples through the fertilization process.",
    "slug": "ForMyFertility",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.7.0",
    "orientation": "portrait",
    "packagerOpts": {
      "nonPersistent": "--nonPersistent"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#45ccb1"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "11",
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.formylife360.formyfertility",
      "splash": {
        "backgroundColor": "#45ccb1",
        "resizeMode": "cover",
        "image": "./assets/splash.png"
      }
    }
  }
}
