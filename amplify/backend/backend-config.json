{
	"auth": {
		"devformyfertilitymobile": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	},
	"api": {
		"formyfertilitymobile": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "AMAZON_COGNITO_USER_POOLS"
			}
		},
		"odds": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "femaleodds",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"storage": {
		"femalefertility": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		},
		"malefertility": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"femaleodds": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "femalefertility",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}