{
    "storage": {
        "femalefertility": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        },
        "malefertility": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "formyfertilityfemaleodds": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "femalefertility",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "auth": {
        "formyfertilitycognito": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": []
        }
    },
    "api": {
        "oddsapi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "formyfertilityfemaleodds",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "formyfertility": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "securityType": "AMAZON_COGNITO_USER_POOLS"
            }
        }
    },
    "analytics": {}
}